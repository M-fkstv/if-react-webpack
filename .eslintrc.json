{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["react-app", "react-app/jest"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "quotes": ["error", "single"],
    "prefer-const": "error",
    "semi": ["error", "always"],
    "no-var": "error",
    "comma-dangle": ["error", "always-multiline"]
  }
}
